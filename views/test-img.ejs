    <h2>
        <%= title %>
    </h2>
    <div id="new-image">
        <img src="/image" alt="" style="max-width:600px">
    </div><br>
    <a href="/" style="max-width:600px; text-align: left; margin-top:10px">ホームに戻る</a>
</body>

